{
  "name": "@banou/stub",
  "version": "0.0.11",
  "license": "MIT",
  "type": "module",
  "main": "build/index.js",
  "_main": "src/index.tsx",
  "keywords": [
    "fkn",
    "fkn-type:app"
  ],
  "files": [
    "build"
  ],
  "devDependencies": {
    "@rollup/plugin-inject": "^5.0.3",
    "@types/bittorrent-protocol": "^3.1.2",
    "@types/simple-peer": "^9.11.5",
    "@vitejs/plugin-react": "^3.0.0",
    "concurrently": "^7.6.0",
    "copyfiles": "^2.4.1",
    "mime": "^3.0.0",
    "node-stdlib-browser": "^1.2.0",
    "nodemon": "^2.0.20",
    "rollup-plugin-polyfill-node": "^0.12.0",
    "shx": "^0.3.4",
    "typescript": "^4.8.3",
    "vite": "^4.1.2"
  },
  "scripts": {
    "dev": "vite --port 4560",
    "build": "vite build && npm run copy-anitomy && npm run copy-media-player",
    "watch-package-build": "nodemon --watch build -e js,wasm,ts --exec \"npm run package-build\"",
    "package-build": "shx rm build/package.asar & npx asar pack build tmp/package.asar && shx mv tmp/package.asar build/package.asar",
    "copy-anitomy": "copyfiles -u 3 ./node_modules/anitomyscript/build/* build",
    "copy-media-player": "copyfiles -u 4 ./node_modules/@banou/media-player/build/* build/tmp && shx rm build/tmp/index.js && shx mv build/tmp/* build && shx rm -rf build/tmp",
    "build-deps": "npm run build-deps-media-player && npm run build-deps-fkn-lib",
    "build-deps-local": "npm run build-deps-media-player && npm run build-deps-fkn-lib-local",
    "build-deps-media-player": "cd node_modules/@banou/media-player && npm i && npm run build && cd ../../..",
    "build-deps-fkn-lib": "cd node_modules/@fkn/lib && npm i && npm run build && cd ../../..",
    "build-deps-fkn-lib-local": "cd node_modules/@fkn/lib && npm i && npm run build-dev && cd ../../..",
    "remove-build-deps": "shx rm -rf node_modules/@banou/media-player"
  },
  "dependencies": {
    "@apollo/client": "^3.7.10",
    "@apollo/server": "^4.4.1",
    "@banou/media-player": "github:Banou26/media-player",
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@fkn/lib": "github:MFKN/fkn-web-lib",
    "@fortawesome/fontawesome-svg-core": "^6.2.1",
    "@fortawesome/free-brands-svg-icons": "^6.2.1",
    "@fortawesome/free-regular-svg-icons": "^6.2.1",
    "@fortawesome/free-solid-svg-icons": "^6.2.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@types/node": "^16.11.6",
    "@types/parse-torrent": "^5.8.4",
    "@types/react": "^17.0.27",
    "anitomyscript": "^2.0.4",
    "buffer": "^6.0.3",
    "dompurify": "^2.3.9",
    "events": "^3.3.0",
    "fp-ts": "^2.12.1",
    "fp-ts-std": "^0.14.2",
    "graphql": "^17.0.0-alpha.1.canary.pr.3361.04ab27334641e170ce0e05bc927b972991953882",
    "graphql-tag": "^2.12.6",
    "idb": "^7.0.0",
    "marked": "^4.0.18",
    "osra": "^0.0.8",
    "p-queue": "^7.3.0",
    "p-throttle": "^5.0.0",
    "parse-torrent": "^9.1.5",
    "path": "^0.12.7",
    "raviger": "^4.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-feather": "^2.0.10",
    "react-hook-form": "^7.34.1",
    "react-tooltip": "^4.2.21",
    "react-use": "^17.4.0",
    "rxjs": "^7.5.5",
    "tslib": "^2.4.1",
    "url": "^0.11.0",
    "util": "^0.12.4"
  }
}
